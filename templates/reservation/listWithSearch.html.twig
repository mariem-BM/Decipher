{{ form(searchReservation) }}
<table border="1">
    <tr>
        <td>ID</td>
        <td>User Email</td>
        <td>Reservation date</td>
        <td>Billet</td>
        <td>Action</td>
    </tr>
    {% for reservation in reservations %}
        <tr>
            <td>{{ reservation.id }}</td>
            <td>{{ reservation.user }}</td>
            <td>{{ reservation.dateReservation ? reservation.dateReservation|date('Y-m-d') : '' }}</td>
            <td>{{ reservation.billet.id }}</td>
            <td><a href="{{ path('reservationfront_show', {'id': reservation.id}) }}">show</a>
                    <a href="{{ path('reservation_edit', {'id': reservation.id}) }}">edit</a></td>
        </tr>
    {% endfor %}
</table>
<a href="{{ path('reservation_new') }}">Create new</a>
